<h2>Clicks for <%= @short_url.short_url %></h2>

<table>
  <thead>
    <tr>
      <th>Geolocation</th>
      <th>Timestamp</th>
    </tr>
  </thead>
  <tbody>
    <% @clicks.each do |click| %>
      <tr>
        <td><%= click.geolocation %></td>
        <td><%= click.clicked_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>
