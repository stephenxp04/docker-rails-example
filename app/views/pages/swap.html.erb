<h1 class="text-2xl font-semibold mb-4">Swap Events for Block <%= 26444465 %></h1>

<% if @swap_events.any? %>
  <div class="flex flex-col space-y-4">
    <% @swap_events.each do |event| %>
      <div class="bg-white shadow-md rounded-lg p-4 border border-gray-200">
        <% event.each do |key, value| %>
          <div class="mb-2 break-words">
            <span class="font-semibold text-gray-700"><%= key.humanize %>:</span>
            <span class="text-gray-900"><%= value %></span>
          </div>
        <% end %>
      </div>
    <% end %>
  </div>
<% else %>
  <p class="text-gray-500">No swap events found for this block.</p>
<% end %>
